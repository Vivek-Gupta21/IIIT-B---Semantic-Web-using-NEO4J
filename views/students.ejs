<h2>Enter the student details</h2>

<div class="fac-text">
<form method="post" action="/student/add">
  <label>Student Roll No: </label>
  <input type="text" name="roll_no"><br><br>
  <label>Student Name: </label>
  <input type="text" name="stu_name"><br><br>
  <label>Student Gender: </label>
  <input type="text" name="stu_gender"><br><br>
  <label>Student CGPA: </label>
  <input type="float" name="cgpa"><br><br>
  <input type="submit" value="Submit">
</form>
</div>


<h2>Add subjects taken by student (hasTaken Relationship)</h2>
<div class="fac-text">
<form method="post" action="/student/takenby/add">
  <label>Student Name: </label>
  <input type="text" name="stu_name"><br><br>
  <label>Subject Name: </label>
  <input type="text" name="Subject_Name"><br><br>

  <input type="submit" value="Submit">
</form>
</div>

<h2>Enrolled in</h2>
<div class="fac-text">
  <form method="post" action="/student/enrolled/add">
  <label>Student Name: </label>
  <input type="text" name="stu_name"><br><br>
  <label>Programme Name: </label>
  <input type="text" name="course_name"><br><br>

  <input type="submit" value="Submit">
</form>
</div>

  <h2>Students Record</h2>
  <table id="customers">
  <tr>
    <th>Roll Number</th>
    <th>Name</th>
    <th>Gender</th>
    <th>CGPA</th>
  </tr>
  <% students.forEach(function(student){ %>
    <tr>
      <td><%= student.id %></td>
      <td><%= student.name %></td>
      <td><%= student.gender %></td>
      <td><%= student.cgpa %></td>
    </tr>
  <% }) %>
  </table>



</body>
</html>
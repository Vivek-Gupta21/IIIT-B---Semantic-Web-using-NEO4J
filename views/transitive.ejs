<body>
  
<h2>Triples with Transitive Property</h2>

<p class="ab" >Student--hasTaken-->Subject o Subject--isTaughtBy-->Faculty =>  Student--isStudentOf-->Faculty</p>

<table id="customers">
  <tr>
    <th>Student</th> 
    <th>Subject</th> 
    <th>Faculty</th> 
  </tr>
  <% Trans.forEach(function(Trans){ %>
  <tr>
    <td><%= Trans.stuName %></td>
    <td><%= Trans.subName %></td>
    <td><%= Trans.facName %></td>
  </tr>
  <% }) %>
</table>


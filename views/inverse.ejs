<body>
  
<h2>Triples with Inverse Property</h2>

<p class="ab">Teaches relationship</p>

<table id="customers">
  <tr>
    <th>Faculty</th> 
    <th>Subject</th> 
  </tr>
  <% teacher.forEach(function(cls){ %>
  <tr>
    <td><%= cls.fac_name %></td>
    <td><%= cls.sub_name %></td>
  </tr>
  <% }) %>
</table>

<p class="ab">Taught By relationship</p>

<table id="customers">
  <tr>
    <th>Subject</th>
    <th>Faculty</th> 
  </tr>
  <% taught_by.forEach(function(cls){ %>
  <tr>
    <td><%= cls.sub_name %></td>
    <td><%= cls.fac_name %></td>
  </tr>
  <% }) %>
</table>




